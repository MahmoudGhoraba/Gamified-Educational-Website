@model List<Spaghetti.Models.TakenAssessment>

@{
    ViewData["Title"] = "Assessment Analysis Results";
}

<h1>Assessment Analysis Results</h1>

@if (Model != null && Model.Count > 0)
{
    <table class="table">
        <thead>
        <tr>
            <th>AssessmentID</th>
            <th>LearnerID</th>
            <th>ScoredPoints</th>
            <!-- Add other properties as needed -->
        </tr>
        </thead>
        <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.AssessmentId</td>
                <td>@item.LearnerId</td>
                <td>@item.ScoredPoints</td>
                <!-- Add other properties as needed -->
            </tr>
        }
        </tbody>
    </table>
}
else
{
    <p>No results found.</p>
}